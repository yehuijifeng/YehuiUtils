<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yehui.utils"
    android:versionCode="100"
    android:versionName="1.0.0">

    <!-- 极光推送  系统要求的权限，必须-->
    <!--注册极光推送消息，前置包名-->
    <!-- Required -->
    <permission
        android:name="com.yehui.utils.permission.JPUSH_MESSAGE"
        android:protectionLevel="signature" />
    <!-- Required -->
    <uses-permission android:name="com.yehui.utils.permission.JPUSH_MESSAGE" />
    <!--接收当前用户-->
    <uses-permission android:name="android.permission.RECEIVE_USER_PRESENT" />
    <!--联网-->
    <uses-permission android:name="android.permission.INTERNET" />
    <!--锁屏-->
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <!--读取手机状态-->
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <!--写sd卡-->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!--读sd卡-->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <!--写设置-->
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />
    <!--震动-->
    <uses-permission android:name="android.permission.VIBRATE" />
    <!--安装卸载文件系统-->
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <!--联网状态-->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <!--wifi状态-->
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <!--系统警告窗口-->
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />

    <!-- 极光推送 可选权限 -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_LOCATION_EXTRA_COMMANDS" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />


    <!--写设置-->
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />
    <!--读sd卡-->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <!--写sd卡-->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!--联网-->
    <uses-permission android:name="android.permission.INTERNET" />
    <!--联网状态-->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <!--wifi状态-->
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <!--锁屏-->
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <!--二维码扫描权限-->
    <!--调用相机权限-->
    <uses-permission android:name="android.permission.CAMERA" />
    <!--硬件相机-->
    <uses-feature android:name="android.hardware.camera" />
    <!--硬件相机自动对焦-->
    <uses-feature android:name="android.hardware.camera.autofocus" />
    <!--震动-->
    <uses-permission android:name="android.permission.VIBRATE" />


    <application
        android:name=".application.YehuiApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!--**********************************极光推送***************************************-->
        <!-- Required -->
        <service
            android:name="cn.jpush.android.service.PushService"
            android:enabled="true"
            android:exported="false">
            <intent-filter>
                <action android:name="cn.jpush.android.intent.REGISTER" />
                <action android:name="cn.jpush.android.intent.REPORT" />
                <action android:name="cn.jpush.android.intent.PushService" />
                <action android:name="cn.jpush.android.intent.PUSH_TIME" />
            </intent-filter>
        </service>

        <!-- Required -->
        <receiver
            android:name="cn.jpush.android.service.PushReceiver"
            android:enabled="true">
            <intent-filter android:priority="1000"> <!--since 1.3.5 -->
                <action android:name="cn.jpush.android.intent.NOTIFICATION_RECEIVED_PROXY" /> <!--since 1.3.5 -->
                <category android:name="com.yehui.utils" /> <!--since 1.3.5 -->
            </intent-filter> <!--since 1.3.5 -->
            <intent-filter>
                <action android:name="android.intent.action.USER_PRESENT" />
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.PACKAGE_ADDED" />
                <action android:name="android.intent.action.PACKAGE_REMOVED" />

                <data android:scheme="package" />
            </intent-filter>
        </receiver>
        <!-- Required SDK核心功能-->
        <activity
            android:name="cn.jpush.android.ui.PushActivity"
            android:configChanges="orientation|keyboardHidden"
            android:theme="@android:style/Theme.Translucent.NoTitleBar">
            <intent-filter>
                <action android:name="cn.jpush.android.ui.PushActivity" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="com.yehui.utils" />
            </intent-filter>
        </activity>
        <!-- Required SDK核心功能-->
        <service
            android:name="cn.jpush.android.service.DownloadService"
            android:enabled="true"
            android:exported="false"/>
        <!-- Required SDK核心功能-->
        <receiver android:name="cn.jpush.android.service.AlarmReceiver" />

        <!-- since 1.8.0 option 可选项。用于同一设备中不同应用的JPush服务相互拉起的功能。 -->
        <!-- 若不启用该功能可删除该组件，将不拉起其他应用也不能被其他应用拉起 -->
        <!--<service
            android:name="cn.jpush.android.service.DaemonService"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="cn.jpush.android.intent.DaemonService" />
                <category android:name="com.yehui.utils" />
            </intent-filter>
        </service>-->

        <!-- JPUSH_CHANNEL 是为了方便开发者统计APK分发渠道。-->
        <!-- 例如: -->
        <!-- 发到 Google Play 的APK可以设置为 google-play; -->
        <!-- 发到其他市场的 APK 可以设置为 xxx-market。 -->
        <!-- 目前这个渠道统计功能的报表还未开放。-->
        <meta-data
            android:name="JPUSH_CHANNEL"
            android:value="developer-default" />
        <!--自己应用在官网申请的key-->
        <meta-data
            android:name="JPUSH_APPKEY"
            android:value="072b61ea14d45b38ae622e16" />
        <!--极光推送必须项，结束-->

        <!--极光推送自定义接受广播设置,指向自定义的广播-->
        <receiver
            android:name="com.yehui.utils.jpush.MyReceiver"
            android:enabled="true">
            <intent-filter>
                <action android:name="cn.jpush.android.intent.REGISTRATION" />
                <action android:name="cn.jpush.android.intent.MESSAGE_RECEIVED" />
                <action android:name="cn.jpush.android.intent.NOTIFICATION_RECEIVED" />
                <action android:name="cn.jpush.android.intent.NOTIFICATION_OPENED" />
                <category android:name="com.yehui.utils" />
            </intent-filter>
        </receiver>

        <!--**********************************极光推送***************************************-->

        <!--主页面-->
        <activity
            android:name=".activity.YehuiHomeActivity"
            android:launchMode="singleTop">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <!--加入购物车动画-->
        <activity
            android:name=".activity.CartAnimationActivity"
            android:launchMode="singleTop">

        </activity>

        <!--list-->
        <activity
            android:name=".activity.ListActivity"
            android:launchMode="singleTop">

        </activity>
        <!--grid-->
        <activity
            android:name=".activity.GridActivity"
            android:launchMode="singleTop">

        </activity>
        <!--瀑布流-->
        <activity
            android:name=".activity.StaggeredActivity"
            android:launchMode="singleTop">

        </activity>
        <!--两级列表-->
        <activity
            android:name=".activity.ExpandableListActivity"
            android:launchMode="singleTop">

        </activity>
        <!--viewpager-->
        <activity
            android:name=".activity.ViewpagerActivity"
            android:launchMode="singleTop">

        </activity>


        <!--数据库操作类-->
        <activity
            android:name=".activity.function.OrmLiteActivity"
            android:launchMode="singleTop">


        </activity>

        <!--提示框-->
        <activity
            android:name=".activity.view.DialogActivity"
            android:launchMode="singleTop">

        </activity>

        <!--剪切图片-->
        <activity
            android:name=".activity.function.ImageCroppingActivity"
            android:launchMode="singleTop" />

        <!--轮番图-->
        <activity
            android:name=".activity.view.ViewFlipperActivity"
            android:launchMode="singleTop">

        </activity>

        <!--popupwindow-->
        <activity
            android:name=".activity.view.PopupwindowActivity"
            android:launchMode="singleTop">


        </activity>

        <!--webview-->
        <activity
            android:name=".activity.view.WebViewActivity"
            android:launchMode="singleTop">

        </activity>

        <!--二维码-->
        <activity
            android:name=".activity.function.ZxingActivity"
            android:launchMode="singleTop">

        </activity>

        <!--二维码扫描区-->
        <activity
            android:name=".view.zxingview.activity.CaptureActivity"
            android:launchMode="singleTop">

        </activity>
        <!--图片查看-->
        <activity
            android:name=".activity.view.PhotoViewActivity"
            android:launchMode="singleTop">

        </activity>

        <!--okhttp网络请求-->
        <activity
            android:name=".activity.function.OkHttpActivity"
            android:launchMode="singleTop">

        </activity>

        <!--极光推送-->
        <activity
            android:name=".activity.function.JPushActivity"
            android:launchMode="singleTop">

        </activity>

        <!--极光推送-->
        <activity
            android:name=".activity.function.UMengActivity"
            android:launchMode="singleTop">

        </activity>

        <!--极光推送-->
        <activity
            android:name=".activity.function.PayActivity"
            android:launchMode="singleTop">

        </activity>

        <!--本地文件操作-->
        <activity
            android:name=".activity.function.FileActivity"
            android:launchMode="singleTop">

        </activity>
    </application>

</manifest>
