<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yehui.utils">
    <!--写设置-->
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />
    <!--读sd卡-->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <!--写sd卡-->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!--联网-->
    <uses-permission android:name="android.permission.INTERNET" />
    <!--联网状态-->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <!--wifi状态-->
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <!--二维码扫描权限-->
    <!--调用相机权限-->
    <uses-permission android:name="android.permission.CAMERA" />
    <!--硬件相机-->
    <uses-feature android:name="android.hardware.camera" />
    <!--硬件相机自动对焦-->
    <uses-feature android:name="android.hardware.camera.autofocus" />
    <!--震动-->
    <uses-permission android:name="android.permission.VIBRATE" />

    <!-- 极光推送  系统要求的权限，必须-->
    <!--注册极光推送消息，前置包名-->
    <uses-permission android:name="com.yehui.utils.permission.JPUSH_MESSAGE" />
    <!--接收当前用户-->
    <uses-permission android:name="com.yehui.utils.permission.RECEIVE_USER_PRESENT" />
    <!--联网-->
    <uses-permission android:name="com.yehui.utils.permission.INTERNET" />
    <!--锁屏-->
    <uses-permission android:name="com.yehui.utils.permission.WAKE_LOCK" />
    <!--读取手机状态-->
    <uses-permission android:name="com.yehui.utils.permission.READ_PHONE_STATE" />
    <!--写sd卡-->
    <uses-permission android:name="com.yehui.utils.permission.WRITE_EXTERNAL_STORAGE" />
    <!--读sd卡-->
    <uses-permission android:name="com.yehui.utils.permission.READ_EXTERNAL_STORAGE" />
    <!--写设置-->
    <uses-permission android:name="com.yehui.utils.permission.WRITE_SETTINGS" />
    <!--震动-->
    <uses-permission android:name="com.yehui.utils.permission.VIBRATE" />
    <!--安装卸载文件系统-->
    <uses-permission android:name="com.yehui.utils.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <!--联网状态-->
    <uses-permission android:name="com.yehui.utils.permission.ACCESS_NETWORK_STATE" />
    <!--wifi状态-->
    <uses-permission android:name="com.yehui.utils.permission.ACCESS_WIFI_STATE" />
    <!--系统警告窗口-->
    <uses-permission android:name="com.yehui.utils.permission.SYSTEM_ALERT_WINDOW" />

    <!-- 极光推送 可选权限 -->
    <uses-permission android:name="com.yehui.utils.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="com.yehui.utils.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="com.yehui.utils.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="com.yehui.utils.permission.ACCESS_LOCATION_EXTRA_COMMANDS" />
    <uses-permission android:name="com.yehui.utils.permission.CHANGE_NETWORK_STATE" />


    <application
        android:name=".application.YehuiApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!--极光推送，必须项-->
        <service
            android:name="cn.jpush.android.service.PushService"
            android:enabled="true"
            android:exported="false">
            <intent-filter>
                <action android:name="cn.jpush.android.intent.REGISTER" />
                <action android:name="cn.jpush.android.intent.REPORT" />
                <action android:name="cn.jpush.android.intent.PushService" />
                <action android:name="cn.jpush.android.intent.PUSH_TIME" />
            </intent-filter>
        </service>

        <receiver
            android:name="cn.jpush.android.service.PushReceiver"
            android:enabled="true">
            <!--优先级-->
            <intent-filter android:priority="1000">
                <action android:name="cn.jpush.android.intent.NOTIFICATION_RECEIVED_PROXY" />   <!--Required  显示通知栏 -->
                <category android:name="com.yehui.utils" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.USER_PRESENT" />
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
            </intent-filter>
            <!-- Optional -->
            <intent-filter>
                <action android:name="android.intent.action.PACKAGE_ADDED" />
                <action android:name="android.intent.action.PACKAGE_REMOVED" />
                <data android:scheme="package" />
            </intent-filter>
        </receiver>
        <!-- Required SDK核心功能-->
        <activity
            android:name="cn.jpush.android.ui.PushActivity"
            android:configChanges="orientation|keyboardHidden"
            android:theme="@android:style/Theme.Translucent.NoTitleBar">
            <intent-filter>
                <action android:name="cn.jpush.android.ui.PushActivity" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="com.yehui.utils" />
            </intent-filter>
        </activity>
        <!-- Required SDK核心功能-->
        <service
            android:name="cn.jpush.android.service.DownloadService"
            android:enabled="true"
            android:exported="false"/>
        <!-- Required SDK核心功能-->
        <receiver android:name="cn.jpush.android.service.AlarmReceiver" />

        <!-- JPUSH_CHANNEL 是为了方便开发者统计APK分发渠道。-->
        <!-- 例如: -->
        <!-- 发到 Google Play 的APK可以设置为 google-play; -->
        <!-- 发到其他市场的 APK 可以设置为 xxx-market。 -->
        <!-- 目前这个渠道统计功能的报表还未开放。-->
        <meta-data
            android:name="JPUSH_CHANNEL"
            android:value="developer-default" />
        <!--自己应用在官网申请的key-->
        <meta-data
            android:name="JPUSH_APPKEY"
            android:value="072b61ea14d45b38ae622e16" />
        <!--极光推送必须项，结束-->

        <!--主页面-->
        <activity
            android:name=".activity.YehuiHomeActivity"
            android:launchMode="singleTop">


        </activity>

        <!--加入购物车动画-->
        <activity
            android:name=".activity.CartAnimationActivity"
            android:launchMode="singleTop">

        </activity>

        <!--viewpager-->
        <activity
            android:name=".activity.ViewpagerActivity"
            android:launchMode="singleTop">

        </activity>

        <!--recyclerview-->
        <activity
            android:name=".activity.RecyclerListActivity"
            android:launchMode="singleTop">

        </activity>

        <!--多级列表-->
        <activity
            android:name=".activity.ExpandableListActivity"
            android:launchMode="singleTop">


        </activity>

        <!--数据库操作类-->
        <activity
            android:name=".activity.SqliteUtilsDemoActivity"
            android:launchMode="singleTop">


        </activity>

        <!--提示框-->
        <activity
            android:name=".activity.DialogActivity"
            android:launchMode="singleTop">

        </activity>

        <!--操作图片-->
        <activity
            android:name=".activity.TouchImageActivity"
            android:launchMode="singleTop">


        </activity>
        <!--剪切图片-->
        <activity
            android:name=".activity.ImageCroppingActivity"
            android:launchMode="singleTop" />

        <!--轮番图-->
        <activity
            android:name=".activity.ViewFlipperActivity"
            android:launchMode="singleTop">

        </activity>

        <!--popupwindow-->
        <activity
            android:name=".activity.PopupwindowActivity"
            android:launchMode="singleTop">


        </activity>

        <!--webview-->
        <activity
            android:name=".activity.WebViewActivity"
            android:launchMode="singleTop">

        </activity>

        <!--二维码-->
        <activity
            android:name=".activity.ZxingActivity"
            android:launchMode="singleTop">


        </activity>

        <!--二维码扫描区-->
        <activity
            android:name=".view.zxingview.activity.CaptureActivity"
            android:launchMode="singleTop">

        </activity>
        <!--图片查看-->
        <activity
            android:name=".activity.PhotoViewActivity"
            android:launchMode="singleTop">

        </activity>

        <!--okhttp网络请求-->
        <activity
            android:name=".activity.OkHttpActivity"
            android:launchMode="singleTop">
        </activity>

        <!--极光推送-->
        <activity
            android:name=".activity.JPushActivity"
            android:launchMode="singleTop">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
